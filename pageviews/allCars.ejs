<%- include('./partials/header.ejs') %>  

    <main>
        <h2>All cars</h2>
        <table>
            <thead>
                <tr>
                    <th>Product Number</th>
                    <th>Model</th>
                    <th>Licence Number</th>
                    <th>Rating</th>
                    <th>Year</th>
                </tr>
            </thead>
            <tbody>
                <% result.forEach(car=>{ %>
                    <tr>
                        <td><%= car.productNumber %></td>
                        <td><%= car.model %></td>
                        <td><%= car.licencePlate %></td>
                        <td><%= car.rating %></td>
                        <td><%= car.year %></td>
                    </tr>
                <% }) %>
            </tbody>
        </table>
        <a href="/"><button>Menu</button></a>
    </main>

<%- include('./partials/footer.ejs') %>